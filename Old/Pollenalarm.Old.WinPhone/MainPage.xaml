                    x:Name="appBarSettings"
                    Click="appBarSettings_Click"
                    Text="Einstellungen" />
                <shell:ApplicationBarMenuItem
                    x:Name="appBarHelp"
                    Click="appBarHelp_Click"
                    Text="Hilfe und Informationen" />
                <!--<shell:ApplicationBarMenuItem Text="TEST: BACKGROUND TASK" x:Name="appBarTestOfflineUpdate" Click="appBarTestOfflineUpdate_Click" />-->
            </shell:ApplicationBar.MenuItems>
            <shell:ApplicationBarIconButton
                x:Name="appBarAddPlace"
                Click="appBarAddPlace_Click"
                IconUri="/Assets/ApplicationBar/appbar.add.png"
                Text="Hinzufügen" />
            <shell:ApplicationBarIconButton
                x:Name="appBarPollenSelection"
                Click="appBarPollenSelection_Click"
                IconUri="/Assets/ApplicationBar/appbar.manage.rest.png"
                Text="Pollen Auswahl" />
            <shell:ApplicationBarIconButton
                x:Name="appBarSearch"
                Click="appBarSearch_Click"
                IconUri="/Assets/ApplicationBar/appbar.feature.search.rest.png"
                Text="Schnell Suche" />
        </shell:ApplicationBar>
    </phone:PhoneApplicationPage.ApplicationBar>

    <!-- LayoutRoot is the root grid where all page content is placed -->
    <Grid
        x:Name="LayoutRoot"
        DataContext="{Binding Current}">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="422*" />
            <RowDefinition Height="62" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>


        <Grid.Background>
            <ImageBrush
                x:Name="BackgroundImage"
                ImageSource="/Assets/Background/PivotBackground2.png"
                Stretch="UniformToFill" />
        </Grid.Background>


        <!-- TitlePanel contains the name of the application and page title -->
        <StackPanel
            x:Name="TitlePanel"
            Grid.Row="0"
            Margin="12,37,0,5"
            RenderTransformOrigin="0.5,0.5">
            <StackPanel.RenderTransform>
                <CompositeTransform />
            </StackPanel.RenderTransform>
            <StackPanel.Projection>
                <PlaneProjection />
            </StackPanel.Projection>
            <StackPanel Orientation="Horizontal">
                <Border
                    Width="64"
                    Height="64"
                    Margin="2,-19,0,-19"
                    Background="{StaticResource PhoneForegroundBrush}">
                    <Border.OpacityMask>
                        <ImageBrush ImageSource="/Assets/Logo-173.png" />
                    </Border.OpacityMask>
                </Border>
                <TextBlock
                    x:Name="ApplicationTitle"
                    Height="Auto"
                    Margin="0,0,12,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center"
                    Style="{StaticResource PhoneTextNormalStyle}"
                    Text="POLLENALARM" />
            </StackPanel>
        </StackPanel>

        <!-- ContentPanel - place additional content here -->
