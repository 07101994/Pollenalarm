            x:Name="ContentPanel"
            Grid.Row="1"
            Grid.RowSpan="3"
            Margin="0"
            RenderTransformOrigin="0.5,0.5">
            <Grid.RenderTransform>
                <CompositeTransform />
            </Grid.RenderTransform>
            <Grid.Projection>
                <PlaneProjection />
            </Grid.Projection>

            <controls:Panorama
                x:Name="pivot"
                Margin="0,-5,0,0"
                Loaded="pivot_Loaded"
                RenderTransformOrigin="0.5,0.5"
                SelectionChanged="pivot_SelectionChanged">
                <controls:PanoramaItem
                    Margin="0"
                    Header="Meine Orte">

                    <ScrollViewer Margin="0,-25,0,0">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>


                            <Button
                                Margin="0,-12,0,0"
                                BorderThickness="0"
                                Padding="0"
                                Style="{Binding Source={StaticResource CustomButtonStyle}}">
                                <StackPanel
                                    x:Name="spnGeneralInformation"
                                    Margin="0,0,0,0"
                                    Orientation="Horizontal"
                                    Tap="spnGeneralInformation_Tap">
                                    <Grid Margin="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>

                                        <TextBlock
                                            Margin="0"
                                            FontFamily="Segoe WP SemiLight"
                                            FontSize="40"
                                            Text="Informationen" />
                                        <TextBlock
                                            Grid.Row="1"
                                            Width="408"
                                            Margin="1,-5,0,0"
                                            FontFamily="Segoe WP"
                                            FontSize="18.667"
                                            Foreground="{StaticResource PhoneBorderBrush}"
